{
  "name": "@hop/engine",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "validate-replay": "npx tsx ./scripts/validateReplay.ts",
