    "check-rng-parity": "node ./scripts/checkRngParity.js",
    "validate-state-mirror": "npx tsx ./scripts/validateStateMirror.ts",
    "validate-state-mirror-sample": "npx tsx ./scripts/validateStateMirror.ts ./scripts/fixtures/mirror.engine.sample.json ./scripts/fixtures/mirror.ui.sample.json"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "~5.9.3",
    "vitest": "^1.1.4"
  }
}
